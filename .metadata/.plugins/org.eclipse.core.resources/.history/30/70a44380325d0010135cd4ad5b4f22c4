#include <Arduino.h>
#define hw D7
void setup() {
  // put your setup code here, to run once:
  Serial.begin(115200);
    pinMode(hw,INPUT);
}

void loop() {
  // put your main code here, to run repeatedly:


 if (digitalRead(hw) == LOW)
  Serial.println("1");
 else
    Serial.println("0");
    delay(100);

}
